{
  "manifest_version": 3,
  "name": "GraphCMS Shortcuts",
  "description": "GraphCMS Shortcuts",
  "options_page": "options.html",
  "icons": {
    "48": "icon-48.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://app.graphcms.com/*"
      ],
      "js": [
        "contentScript.bundle.js"
      ],
      "css": [
        "content.styles.css"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "content.styles.css",
        "icon-48.png",
        "icon-48.png"
      ],
      "matches": []
    }
  ]
}